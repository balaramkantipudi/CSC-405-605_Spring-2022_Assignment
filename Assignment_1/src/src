# Make the list with the number of people in the 
for i in range(0, count):
    l.append(int(classsize/count))

count1 = classsize%count
r = 0

while count1 > 0:
    l[r] = l[r] + 1
    r = r + 1
    count1 = count1 - 1

for i in range(0, count):
    stu = random.sample(students, l[i])
    print(stu)
    
    for element in stu:
        students.remove(element)
    
    stu.clear()
